<h1>Listing PostedFiles</h1>

<table>
  <tr>
    <th>KlassType</th>
    <th>KlassId</th>
    <th>OriginalFileName</th>
    <th>FileType</th>
    <th>CreatedAt</th>
  </tr>

<% for uppity_file in @posted_files %>
  <tr>
    <td><%= uppity_file.klass_type %></td>
    <td><%= uppity_file.klass_id %></td>
    <td><%= uppity_file.original_file_name %></td>
    <td><%= uppity_file.file_type %></td>
    <td><%= uppity_file.created_at %></td>
    <td><%= link_to("Show", posted_files_show_url(:id => uppity_file)) %></td>
    <td><%= link_to("Edit", posted_files_edit_url(:id => uppity_file)) %></td>
    <td><%= link_to("Delete", posted_files_delete_url(:id => uppity_file), :method => :delete, :confirm => "Are you sure?") %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to("New PostedFile", posted_files_new_url) %>
